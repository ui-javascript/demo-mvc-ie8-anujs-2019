{
  "name": "sanframeworkie8",
  "version": "0.0.1",
  "description": "webpack2编译打包支持到低版本IE。坑有多少？水有多深？自行体会！",
  "author": {
    "name": "QingHuan",
    "email": "139966880@qq.com",
    "url": "https://github.com/QingHuanMoon"
  },
  "license": "ISC",
  "scripts": {
    "install:cnpm": "cnpm install",
    "remove": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "start": "better-npm-run dev --open",
    "dev": "better-npm-run dev",
    "build:dist": "better-npm-run build",
    "deploy": "npm run clean && npm run build",
    "test": "better-npm-run test",
    "test:dev": "better-npm-run test:dev",
    "test:coveralls": "cat ./build/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "visualizer": "better-npm-run visualizer",
    "report-coverage": "codecov",
    "installKarma": "npm install mocha chai karma karma-coverage karma-mocha karma-webpack karma-phantomjs-launcher phantomjs-prebuilt"
  },
  "betterScripts": {
    "dev": {
      "command": "node ./build/dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config build/webpack.prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "production",
        "NODE_TEST": "production"
      }
    },
    "test:dev": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "production",
        "NODE_TEST": "development"
      }
    },
    "visualizer": {
      "command": "webpack --config build/webpack.prod",
      "env": {
        "NODE_ENV": "production",
        "NODE_TEST": "production"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/sayll/ie-webpack-start/issues",
    "email": "kilol@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sayll/ie-webpack-start.git"
  },
  "homepage": "https://github.com/sayll/ie-webpack-start#readme",
  "dependencies": {
    "avalon2": "^2.2.4",
    "es5-shim": "^4.5.9",
    "prerender-spa-plugin": "^3.0.0",
    "regularjs": "^0.6.0-beta.10",
    "san-store": "^1.1.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
