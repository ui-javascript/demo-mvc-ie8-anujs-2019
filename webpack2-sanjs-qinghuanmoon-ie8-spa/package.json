    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "better-npm-run": "0.0.14",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.26.2",
    "es3ify-loader": "^0.2.0",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "glob": "^7.1.1",
    "happypack": "^3.0.3",
    "html-loader": "^0.4.5",
