    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "san": "^3.5.1",
    "san-loader": "0.0.7",
    "san-router": "^1.2.0",
    "style-loader": "^0.13.2",
